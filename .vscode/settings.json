// Place your settings in this file to overwrite default and user settings.
{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/build/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/build/**": true
  },
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "editor.tabSize": 2
}